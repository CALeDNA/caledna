<% content_for(:title) { 'Import metadata from eDNA results' } %>

<header class="main-content__header" role="banner">
  <h1 class="main-content__page-title">
    <%= content_for(:title) %>
  </h1>
</header>

<section class="main-content__body">
  <p>Upload CSV to import taxonomy from eDNA results.</p>

  <%= form_tag admin_labwork_import_edna_results_metadata_path, multipart: true do %>
    <%= select(:dna_results, :research_project_id, @projects, {include_blank: 'Select research project'}) %>
    <%= file_field_tag :file %>
    <%= submit_tag "Import" %>
  <% end %>
</section>
