<% content_for(:title) { 'Normalize Taxonomy from DNA Results' } %>

<header class="main-content__header" role="banner">
  <h1 class="main-content__page-title">
    <%= content_for(:title) %>
  </h1>
</header>

<section class="main-content__body">
  <table  class="no-auto-link">
    <tr>
      <th>rank</th>
      <th>eDNA results</th>
      <th></th>
    <tr>
    <% @taxa.each do |taxon| %>
    <tr>
      <td><%= taxon.taxon_rank %></td>
      <td><%= taxon.original_taxonomy_string %></td>
      <td>
        <%= link_to 'Edit', admin_labwork_normalize_ncbi_taxon_path(taxon) %>
      </td>
    </tr>
    <% end %>
  </table>
</section>
<%= paginate @taxa %>
