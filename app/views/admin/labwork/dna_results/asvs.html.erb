<% content_for(:title) { 'Import ASV form DNA Results' } %>

<header class="main-content__header" role="banner">
  <h1 class="main-content__page-title">
    <%= content_for(:title) %>
  </h1>
</header>

<section class="main-content__body">
  <p>Upload CSV to import ASVs from DNA test results.</p>

  <%= form_tag admin_labwork_asvs_path, multipart: true do %>
    <%= select(:dna_results, :research_project_id, @projects, {include_blank: 'Select research project'}) %>
    <%= select(:dna_results, :extraction_type_id, @extraction_types, {include_blank: 'Select extraction type'}) %>
    <%= file_field_tag :file %>
    <%= submit_tag "Import" %>
  <% end %>
</section>
